{
  "HTML5 Template": {
    "prefix": "html5",
    "body": [
      "doctype html",
      "html(lang=\"${1:en}\")",
      "  head",
      "    meta(charset=\"utf-8\")",
      "    meta(name=\"viewport\" content=\"width=device-width, initial-scale=1\")",
      "    title ${2:Page Title}",
      "  body",
      "    $0"
    ],
    "description": "Complete HTML5 template"
  },
  "Doctype": {
    "prefix": "doctype",
    "body": "doctype html",
    "description": "HTML5 doctype"
  },
  "Div with class": {
    "prefix": "div.",
    "body": [
      "div.${1:classname}",
      "  $0"
    ],
    "description": "Div with class"
  },
  "Div with ID": {
    "prefix": "div#",
    "body": [
      "div#${1:id}",
      "  $0"
    ],
    "description": "Div with ID"
  },
  "Link": {
    "prefix": "a",
    "body": "a(href=\"${1:#}\") ${2:Link text}",
    "description": "Anchor link"
  },
  "Image": {
    "prefix": "img",
    "body": "img(src=\"${1:image.jpg}\" alt=\"${2:Alt text}\")",
    "description": "Image tag"
  },
  "Input": {
    "prefix": "input",
    "body": "input(type=\"${1:text}\" name=\"${2:name}\"${3: placeholder=\"${4:Placeholder}\"})",
    "description": "Input field"
  },
  "Button": {
    "prefix": "button",
    "body": "button(type=\"${1:button}\") ${2:Button text}",
    "description": "Button element"
  },
  "If statement": {
    "prefix": "if",
    "body": [
      "if ${1:condition}",
      "  $0"
    ],
    "description": "If conditional"
  },
  "If-else statement": {
    "prefix": "ifelse",
    "body": [
      "if ${1:condition}",
      "  ${2}",
      "else",
      "  $0"
    ],
    "description": "If-else conditional"
  },
  "Unless statement": {
    "prefix": "unless",
    "body": [
      "unless ${1:condition}",
      "  $0"
    ],
    "description": "Unless conditional"
  },
  "Each loop": {
    "prefix": "each",
    "body": [
      "each ${1:item} in ${2:items}",
      "  $0"
    ],
    "description": "Each loop"
  },
  "Mixin definition": {
    "prefix": "mixin",
    "body": [
      "mixin ${1:name}(${2:args})",
      "  $0"
    ],
    "description": "Define a mixin"
  },
  "Mixin call": {
    "prefix": "+",
    "body": "+${1:mixinName}(${2:args})",
    "description": "Call a mixin"
  },
  "Interpolation": {
    "prefix": "#{",
    "body": "#{${1:expression}}",
    "description": "JavaScript interpolation"
  },
  "Block": {
    "prefix": "block",
    "body": [
      "block ${1:name}",
      "  $0"
    ],
    "description": "Define a block"
  },
  "Extends": {
    "prefix": "extends",
    "body": "extends ${1:layout}",
    "description": "Extend a layout"
  },
  "Include": {
    "prefix": "include",
    "body": "include ${1:file}",
    "description": "Include a file"
  },
  "Script tag": {
    "prefix": "script",
    "body": "script(src=\"${1:script.js}\")",
    "description": "Script tag"
  },
  "Style tag": {
    "prefix": "style",
    "body": [
      "style.",
      "  ${1:/* CSS */}",
      "  $0"
    ],
    "description": "Style tag with CSS"
  },
  "Meta tag": {
    "prefix": "meta",
    "body": "meta(name=\"${1:description}\" content=\"${2:Content}\")",
    "description": "Meta tag"
  },
  "List": {
    "prefix": "ul>li",
    "body": [
      "ul",
      "  li ${1:Item 1}",
      "  li ${2:Item 2}",
      "  li ${3:Item 3}",
      "  $0"
    ],
    "description": "Unordered list with items"
  },
  "Table": {
    "prefix": "table",
    "body": [
      "table",
      "  thead",
      "    tr",
      "      th ${1:Header 1}",
      "      th ${2:Header 2}",
      "  tbody",
      "    tr",
      "      td ${3:Cell 1}",
      "      td ${4:Cell 2}",
      "    $0"
    ],
    "description": "Table with header and body"
  },
  "Form": {
    "prefix": "form",
    "body": [
      "form(action=\"${1:#}\" method=\"${2:post}\")",
      "  $0"
    ],
    "description": "Form element"
  }
}
